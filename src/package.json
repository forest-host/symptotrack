{
  "name": "symptotrack",
  "version": "1.0.0",
  "description": "Frontend for symptotrack.org",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "npm run dev",
    "serve": "NODE_ENV=production node server.js",
    "server": "npm run serve",
    "test": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --ignore-path dist --write",
      "git add"
    ]
  },
  "dependencies": {
    "@symptotrack/questions": "^0.6.20",
    "@xstyled/styled-components": "^1.16.0",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "compression": "^1.7.4",
    "core-js": "^3.1.4",
    "express": "^4.17.1",
    "leaflet": "^1.6.0",
    "leaflet-geosearch": "^2.7.0",
    "next": "^9.3.1",
    "next-i18next": "^4.2.1",
    "next-offline": "^5.0.0",
    "next-optimized-images": "^2.5.3",
    "nookies": "^2.0.8",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "prettier": "^1.18.0",
    "react": "^16.13.1",
    "react-div-100vh": "^0.3.8",
    "react-dom": "16.13.1",
    "react-hook-form": "^5.1.3",
    "react-leaflet": "^2.6.3",
    "react-rangeslider": "^2.2.0",
    "react-select": "^3.1.0",
    "styled-components": "^4.3.2",
    "styled-normalize": "^8.0.6",
    "styled-system": "^5.0.15",
    "webp-loader": "^0.5.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "html-react-parser": "^0.9.2",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "react-axe": "^3.1.0",
    "react-openlayers": "^0.3.0",
    "react-test-renderer": "^16.8.1",
    "uuid": "^3.3.2"
  }
}
